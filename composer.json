{
    "name": "victoryoalli/lw-survey",
    "description": "Laravel Livewire Survey PHP Package",
    "keywords": [
        "victoryoalli",
        "livewire",
        "laravel",
        "survey"
    ],
    "homepage": "https://github.com/victoryoalli/lw-survey",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Victor Yoalli Dominguez",
            "email": "victoryoalli@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^7.0",
        "livewire/livewire": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "VictorYoalli\\LwSurvey\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VictorYoalli\\LwSurvey\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "VictorYoalli\\LwSurvey\\LwSurveyServiceProvider"
            ],
            "aliases": {
                "LwSurvey": "VictorYoalli\\LwSurvey\\LwSurveyFacade"
            }
        }
    }
}